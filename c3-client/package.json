{
  "name": "c3-client",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "api:download-and-generate": "ts-node ./openapi/download-api-spec.ts && npm run-script api:generate-from-existing-spec",
    "api:generate-from-existing-spec": "npx rimraf src/app/core/api/v1 && openapi-generator-cli generate -i ./openapi/api-spec.json -g typescript-angular -o src/app/core/api/v1 -c openapi/openapi-generator.yaml"
  },
  "main": "electron-app/main.js",
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.0",
    "@angular/common": "~13.3.0",
    "@angular/compiler": "~13.3.0",
    "@angular/core": "~13.3.0",
    "@angular/forms": "~13.3.0",
    "@angular/platform-browser": "~13.3.0",
    "@angular/platform-browser-dynamic": "~13.3.0",
    "@angular/router": "~13.3.0",
    "@types/offscreencanvas": "^2019.6.4",
    "pixi-particles": "^4.3.1",
    "pixi.js": "^6.3.2",
    "rxjs": "~7.5.0",
    "socket.io-client": "^4.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.0",
    "@angular/cli": "~13.3.0",
    "@angular/compiler-cli": "~13.3.0",
    "@openapitools/openapi-generator-cli": "^2.5.1",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.20.52",
    "electron": "^18.2.3",
    "electron-builder": "^23.0.3",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "rimraf": "^3.0.2",
    "tsconfig-paths": "^4.0.0",
    "typescript": "~4.6.2"
  },
  "build": {
    "appId": "c3-client",
    "files": [
      "angular-build/**/*",
      "electron-app/main.js"
    ],
    "win": {
      "icon": "./angular-build/assets/img/icon.png",
      "target": [
        "portable"
      ]
    },
    "mac": {
      "icon": "./angular-build/assets/img/icon.png",
      "target": [
        "dmg"
      ]
    },
    "linux": {
      "icon": "./angular-build/assets/img/icon.png",
      "target": [
        "AppImage"
      ]
    }
  }
}
